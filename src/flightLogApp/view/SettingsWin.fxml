<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ScrollPane prefHeight="300.0" prefWidth="300.0">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0">
               <children>
                  <AnchorPane AnchorPane.leftAnchor="5.0" AnchorPane.topAnchor="10.0" />
                  <AnchorPane layoutX="230.0" layoutY="30.0" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0">
                     <children>
                        <Label text="Language (restart required)" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="7.0">
                           <font>
                              <Font size="13.0" />
                           </font>
                        </Label>
                        <ChoiceBox fx:id="languageChoice" prefWidth="150.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="25.0" />
                     </children>
                  </AnchorPane>
                  <AnchorPane layoutX="15.0" layoutY="20.0" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="70.0">
                     <children>
                        <Label text="Theme" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="7.0">
                           <font>
                              <Font size="13.0" />
                           </font>
                        </Label>
                        <ChoiceBox fx:id="themeChoice" prefWidth="150.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="25.0" />
                     </children>
                  </AnchorPane>
                  <AnchorPane AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="200.0">
                     <children>
                        <Button fx:id="saveBut" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#saveButton" text="save" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" />
                        <Button fx:id="cancelBut" layoutX="150.0" layoutY="10.0" mnemonicParsing="false" onAction="#cancelButton" text="Cancel" AnchorPane.leftAnchor="80.0" AnchorPane.topAnchor="0.0" />
                        <Label fx:id="errorLabel" text="Label" textFill="RED" visible="false" AnchorPane.leftAnchor="5.0" AnchorPane.topAnchor="40.0" />
                     </children>
                  </AnchorPane>
               </children>
            </AnchorPane>
        </content>
      </ScrollPane>
   </children>
</AnchorPane>
